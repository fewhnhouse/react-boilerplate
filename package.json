{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "My personal boilerplate for react apps using webpack and babel.",
  "main": "index.js",
  "author": "Felix Wohnhaas",
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "react-hot-loader": "^3.1.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
