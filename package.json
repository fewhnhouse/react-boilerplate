{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "My personal boilerplate for react apps using webpack and babel.",
  "main": "index.js",
  "author": "Felix Wohnhaas",
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "file-loader": "^1.1.9",
    "react-hot-loader": "^3.1.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --mode development --config ./webpack.dev.js --open",
    "remote": "webpack-dev-server --host 0.0.0.0 --public fewhnhouse.de:5000 --port 5000 --progress --colors --config ./webpack.dev.js",
    "clean": "node scripts/setup.js",
    "build": "webpack --mode production --config ./webpack.prod.js"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "shelljs": "^0.8.1",
    "styled-components": "^3.1.6"
  }
}
